<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ko≈°arica</title>
    <link rel="stylesheet" href="/styles/stylecart.css">

</head>
<body>

<div class="sadrzaj">
    <%- include('partials/header', { openCategory: openCategory, basketCount: basketCount }) %>

    <div class="ispod_gornjeg_djkela">

        <div>
            <div class="bigPicture">
                <img height="250px" src="/images/l2.jpg" alt="">
            </div>
        </div>

    </div>

    <div class="centar">
        <div class="content">
            <% cartName.forEach((value, key) => {
            if(cartCount.get(key) == 0) return;
            %>
            <div class="proizvod">
                <span><%=value%></span>
                <button class="remove" onclick="removeFromCart('<%=key%>')">-</button>
                <a id="<%=key%>"><%=cartCount.get(key)%></a>
                <button class="add" onclick="addToCart('<%=key%>')">+</button>
            </div>
            <% }) %>
        </div>
    </div>


</div>

<script src="/scripts/script.js"></script>
</body>
</html>